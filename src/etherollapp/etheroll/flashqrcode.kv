#:import platform kivy.utils.platform
#:import ZBarSymbol pyzbar.pyzbar.ZBarSymbol


<FlashQrCodeScreen>:
    on_pre_enter:
        zbarcam_id.start()
        self.bind_on_symbols()
    on_pre_leave:
        zbarcam_id.stop()
        self.unbind_on_symbols()
    ZBarCam:
        id: zbarcam_id
        code_types: ZBarSymbol.QRCODE,
